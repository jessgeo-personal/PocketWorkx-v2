{
  "name": "pocketworkx-v2",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:dev": "EXPO_PUBLIC_ENV=development expo start",
    "start:staging": "EXPO_PUBLIC_ENV=staging expo start",
    "start:prod": "EXPO_PUBLIC_ENV=production expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev": "EXPO_PUBLIC_ENV=development expo build",
    "build:staging": "EXPO_PUBLIC_ENV=staging expo build",
    "build:prod": "EXPO_PUBLIC_ENV=production expo build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "detox test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "setup:dev": "npm run setup:env:dev && npm install",
    "setup:staging": "npm run setup:env:staging && npm install",
    "setup:prod": "npm run setup:env:prod && npm install",
    "setup:env:dev": "cp .env.development .env",
    "setup:env:staging": "cp .env.staging .env",
    "setup:env:prod": "cp .env.production .env"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@expo-google-fonts/inter": "^0.2.3",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "expo": "~50.0.0",
    "expo-auth-session": "~5.4.0",
    "expo-build-properties": "~0.11.1",
    "expo-camera": "~14.1.0",
    "expo-constants": "~15.4.0",
    "expo-crypto": "~12.8.0",
    "expo-document-picker": "~11.10.0",
    "expo-file-system": "~16.0.0",
    "expo-font": "~11.10.0",
    "expo-image-picker": "~14.7.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-router": "~3.4.0",
    "expo-secure-store": "~12.8.0",
    "expo-sharing": "~11.10.0",
    "expo-sms": "~11.7.0",
    "expo-status-bar": "~1.11.0",
    "expo-web-browser": "~12.8.0",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.5.8",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.13.5",
    "eslint": "^8.54.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{js,ts}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "private": true
}