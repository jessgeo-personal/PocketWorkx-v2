# Staging Environment Configuration

EXPO_PUBLIC_ENV=staging
EXPO_PUBLIC_APP_NAME=PocketWorkx Staging
EXPO_PUBLIC_BUNDLE_ID=com.pocketworkx.app.staging
EXPO_PUBLIC_VERSION=1.0.0-staging

# API Configuration
EXPO_PUBLIC_API_URL=https://staging-api.pocketworkx.com
EXPO_PUBLIC_API_TIMEOUT=15000

# Debug and Logging
EXPO_PUBLIC_ENABLE_DEBUG=true
EXPO_PUBLIC_LOG_LEVEL=info

# Google Drive Integration
EXPO_PUBLIC_GOOGLE_DRIVE_SCOPE=staging
EXPO_PUBLIC_GOOGLE_CLIENT_ID=staging-google-client-id

# Security
EXPO_PUBLIC_ENCRYPTION_KEY_VERSION=1
EXPO_PUBLIC_ENABLE_BIOMETRIC=true

# Analytics
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_CRASH_REPORTING=true

# Feature Flags
EXPO_PUBLIC_ENABLE_CRYPTO=false
EXPO_PUBLIC_ENABLE_ADVANCED_ANALYTICS=false
EXPO_PUBLIC_ENABLE_MULTI_CURRENCY=true
